{
  "name": "What's in the box",
  "version": "0.0.1",
  "main": "public/app.js",
  "authors": [
    "Christian de Botton <cdebotton@gmail.com>"
  ],
  "description": "A brunch scaffold for complex Backbone apps.",
  "keywords": [
    "brunch",
    "backbone",
    "stylus",
    "relational",
    "scaffolt",
    "emblem"
  ],
  "license": "MIT",
  "homepage": "http://www.christiandebotton.com",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "app/bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "backbone": "~1.1.0",
    "jquery": "~2.0.3",
    "backbone-relational": "~0.8.6",
    "handlebars.js": "~1.1.2",
    "font-awesome": "~4.0.3",
    "swag": "~0.5.0",
    "modernizr": "~2.7.1",
    "normalize-css": "~2.1.3",
    "underscore": "~1.5.2",
    "backbone-validation": "~0.9.0"
  },
  "devDependencies": {
    "emblem.js": "*",
    "chai": "~1.8.1",
    "chai-jquery": "~1.1.2",
    "chai-changes": "*",
    "chai-backbone": "*",
    "sinon-chai": "~2.4.0",
    "sinonjs": "~1.7.3"
  },
  "overrides": {
    "underscore": {
      "main": "underscore.js"
    },
    "backbone": {
      "main": "backbone.js",
      "dependencies": {
        "jquery": "*",
        "underscore": "*"
      }
    },
    "backbone-validation": {
      "main": [
        "dist/backbone-validation.js"
      ],
      "dependencies": {
        "backbone": "*"
      }
    },
    "handlebars.js": {
      "main": "handlebars.js"
    },
    "backbone-relational": {
      "main": "backbone-relational.js",
      "dependencies": {
        "backbone": "*"
      }
    },
    "font-awesome": {
      "main": "css/font-awesome.css"
    },
    "modernizr": {
      "main": "modernizr.js"
    }
  }
}
